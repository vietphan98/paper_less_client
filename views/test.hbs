<!-- CSS -->
<style>
 #my_camera{
     width: 320px;
     height: 240px;
     border: 1px solid black;
}
</style>

<div id="my_camera"></div>
<input type=button value="Take Snapshot" onClick="take_snapshot()">
<input type=button value="CS FORM LAYOUT" onClick="submit_cs_form()">

 
<div id="results" ></div>
 
<!-- Webcam.min.js -->
<script type="text/javascript" src="webcamjs/webcam.min.js"></script>

<!-- Configure a few settings and attach camera -->
<script language="JavaScript">
 Webcam.set({
     width: 320,
     height: 240,
     image_format: 'jpeg',
     jpeg_quality: 90
 });
 Webcam.attach( '#my_camera' );

<!-- Code to handle taking the snapshot and displaying it locally -->
function take_snapshot() {
 
   // take snapshot and get image data
   Webcam.snap( function(data_uri) {
       // display results in page
       document.getElementById('results').innerHTML = 
        '<img src="'+data_uri+'"/>';
    } );
}
function submit_cs_form(){
    let urlSend = "https://zeros.asia:5656/api/layout_pdf_cs_form/";
        let result = AjaxAsync(urlSend,{
            "SOLINE":"test",
    },"GET", "JSON");
    if(result.code === 200){
      window.open("./test.pdf", "width=500,height=500,top=100,left=500")
    }
      let result_delete = AjaxAsync(urlSend + 'delete/',{
                "SOLINE":"test",
     },"GET", "JSON");
     
   
}
</script>
